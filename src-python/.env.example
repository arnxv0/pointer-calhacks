# Google API Keys (REQUIRED)
GOOGLE_GENAI_USE_VERTEXAI=0
GOOGLE_API_KEY=your_google_api_key_here
GROQ_API_KEY=your_groq_api_key_here

# Email (SMTP) - Optional for email functionality
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_email@gmail.com
SMTP_PASSWORD=your_app_password
SMTP_FROM=your_email@gmail.com

# Google Calendar - Optional for calendar functionality
GOOGLE_CALENDAR_CREDENTIALS_JSON=./credentials.json
GOOGLE_CALENDAR_TOKEN_JSON=./token.json
CALENDAR_ID=primary

# Pointer Configuration
POINTER_APP_NAME=pointer
POINTER_AGENTS_DIR=./agents
POINTER_ENV_DIR=./
